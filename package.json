{
    "name": "logger",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "jest": "jest",
        "start": "ng serve",
        "copy-readme": "ts-node --project tsconfig.tools.json ./tools/copy-readme",
        "build": "ng build logger && npm run copy-readme",
        "build.meta": "echo 'prebuild'",
        "test": "jest --config ./jest.app.config.js --coverage",
        "lint": "ng lint",
        "format": "prettier --write \"*/**/*.ts\"",
        "coverage": "jest --config ./jest.app.config.js --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.11",
        "@angular/common": "~8.2.11",
        "@angular/compiler": "~8.2.11",
        "@angular/core": "~8.2.11",
        "@angular/forms": "~8.2.11",
        "@angular/platform-browser": "~8.2.11",
        "@angular/platform-browser-dynamic": "~8.2.11",
        "@angular/router": "~8.2.11",
        "core-js": "^3.3.3",
        "devtools-detect": "^3.0.0",
        "lint-staged": "^9.4.2",
        "rxjs": "~6.5.3",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.12",
        "@angular-devkit/build-ng-packagr": "~0.803.12",
        "@angular/cli": "~8.3.12",
        "@angular/compiler-cli": "~8.2.11",
        "@angular/language-service": "~8.2.11",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "~8.9.4",
        "codelyzer": "^4.5.0",
        "coveralls": "^3.0.7",
        "husky": "^2.7.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^24.9.0",
        "jest-cli": "^24.9.0",
        "jest-preset-angular": "^7.1.1",
        "karma": "~4.4.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^5.7.0",
        "prettier": "^1.18.2",
        "protractor": "~5.4.2",
        "ts-node": "^7.0.1",
        "tsickle": ">=0.37.0",
        "tslib": "^1.10.0",
        "tslint": "~5.20.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-sonarts": "^1.9.0",
        "typescript": "~3.5.3"
    }
}
